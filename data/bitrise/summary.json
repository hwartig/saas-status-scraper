  "scheduled_maintenances": [
    {
      "id": "td243swg9nv8",
      "name": "Key-Value Cache Reset for Linux VMs",
      "status": "scheduled",
      "created_at": "2024-10-16T15:05:55.248Z",
      "updated_at": "2024-10-16T15:05:55.640Z",
      "monitoring_at": null,
      "resolved_at": null,
      "impact": "maintenance",
      "shortlink": "https://stspg.io/t9jggdy79rcn",
      "started_at": "2024-10-16T15:05:55.242Z",
      "page_id": "1q8r4n02vb2k",
      "incident_updates": [
        {
          "id": "gfsq51rk2844",
          "status": "scheduled",
          "body": "On Monday, October 21st, at 11 AM CEST, we will be implementing infrastructure changes to improve the performance and reliability of our Key-Value Cache for Linux virtual machines. As part of this update, we will reset customer cache data.\nAs a user of our Key-Value Caching, your first build following this change will run without utilizing the cache, which may result in slightly longer build times. However, your cache will be repopulated automatically, and subsequent builds will once again benefit from the usual performance enhancements.\nNo action is required on your part, and we anticipate minimal disruption to your workflow. This change will not affect Mac virtual machines.",
          "incident_id": "td243swg9nv8",
