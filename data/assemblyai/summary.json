      "resolved_at": null,
      "impact": "major",
      "shortlink": "https://stspg.io/8tvz9yml6h0b",
      "started_at": "2025-08-05T16:05:14.486Z",
      "page_id": "8yxng9m82rr4",
      "incident_updates": [
        {
          "id": "nvd8985y6pt8",
          "status": "monitoring",
          "body": "We have resolved the issue that was affecting Slam-1 and are now seeing requests being processed successfully. We will continue to monitor traffic to ensure good performance, but everything is working as expected at the moment.",
          "incident_id": "rz51jtsbvhcq",
          "created_at": "2025-08-05T16:23:46.215Z",
          "updated_at": "2025-08-05T16:23:46.215Z",
